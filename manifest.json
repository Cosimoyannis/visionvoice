{
    "manifest_version": 3,
    "name": "VisionVoice Screenreader",
    "version": "1.0",
    "description": "A Chrome extension to capture a screenshot and analyze it using AI.",
    "permissions": [
      "activeTab",
      "scripting",
      "tabs"
    ],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "32": "Logo.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  